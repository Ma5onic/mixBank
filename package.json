{
  "name": "mixBank",
  "version": "1.0.0",
  "description": "Totally Sweet App",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "testdev": "browserify test/test.js | node | tap-spec",
    "test": "node test/supertest.js | tap-diff",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mandy-kate/mixBank"
  },
  "author": "mandy-kate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mandy-kate/mixBank/issues"
  },
  "homepage": "https://github.com/mandy-kate/mixBank",
  "dependencies": {
    "express": "^4.14.0",
    "findup-sync": "^0.4.2",
    "knex": "^0.11.10",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "supertest": "^2.0.0",
    "tap-diff": "^0.1.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
