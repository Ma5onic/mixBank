{
  "name": "mixBank",
  "version": "1.0.0",
  "description": "Totally Sweet App",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run bundle",
    "bundle": "NODE_ENV=production browserify client/index.js -o public/bundle.js -g uglifyify",
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "db:setup": "knex migrate:latest && knex seed:run",
    "dev:watch": "watchify client/index.js -o public/bundle.js -d",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run bundle && npm run dist && npm run push",
    "testdev": "browserify test/test.js | node | tap-spec",
    "test": "node ./test/*.js | tap-spec"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mandy-kate/mixBank"
  },
  "author": "mandy-kate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mandy-kate/mixBank/issues"
  },
  "homepage": "https://github.com/mandy-kate/mixBank",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-hbs": "^1.0.3",
    "express-session": "^1.14.1",
    "findup-sync": "^0.4.2",
    "hbs": "~4.0.0",
    "knex": "^0.11.10",
    "latest-version": "^2.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.2.0",
    "supertest": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "nodemon": "^1.10.2"
  }
}
